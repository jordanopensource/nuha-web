# Session Configuration
#samplr#NUXT_SESSION_NAME=auth_session
NUXT_SESSION_NAME=auth_session
#samplr#NUXT_SESSION_PASSWORD=your-session-password-with-at-least-32-characters-here
NUXT_SESSION_PASSWORD=your-session-password-with-at-least-32-characters-here

# OAuth GitHub Configuration (Get from GitHub App settings)
#samplr#NUXT_OAUTH_GITHUB_CLIENT_ID=your-github-client-id
NUXT_OAUTH_GITHUB_CLIENT_ID=your-github-client-id
#samplr#NUXT_OAUTH_GITHUB_CLIENT_SECRET=your-github-client-secret
NUXT_OAUTH_GITHUB_CLIENT_SECRET=your-github-client-secret

# OAuth Google configs
# https://console.cloud.google.com/apis/credentials
#samplr#NUXT_OAUTH_GOOGLE_CLIENT_ID=your-google-client-id
NUXT_OAUTH_GOOGLE_CLIENT_ID=your-google-client-id
#samplr#NUXT_OAUTH_GOOGLE_CLIENT_SECRET=your-google-client-secret
NUXT_OAUTH_GOOGLE_CLIENT_SECRET=your-google-client-secret

# Login Options Visibility (set to 'false' to hide, enabled by default)
#samplr#NUXT_PUBLIC_AUTH_EMAIL_ENABLED=true
NUXT_PUBLIC_AUTH_EMAIL_ENABLED=true
#samplr#NUXT_PUBLIC_AUTH_GITHUB_ENABLED=true
NUXT_PUBLIC_AUTH_GITHUB_ENABLED=true
#samplr#NUXT_PUBLIC_AUTH_GOOGLE_ENABLED=true
NUXT_PUBLIC_AUTH_GOOGLE_ENABLED=true

# Listmonk Configuration (for sending magic link emails)
#samplr#NUXT_LISTMONK_URL=http://localhost:9000
NUXT_LISTMONK_URL=http://localhost:9000
#samplr#NUXT_LISTMONK_USER=listmonk-username
NUXT_LISTMONK_USER=listmonk-username
#samplr#NUXT_LISTMONK_TOKEN=listmonk-token
NUXT_LISTMONK_TOKEN=listmonk-token
#samplr#NUXT_LISTMONK_LIST_ID=
NUXT_LISTMONK_LIST_ID=
#samplr#NUXT_LISTMONK_EN_TEMPLATE_ID=
NUXT_LISTMONK_EN_TEMPLATE_ID=
#samplr#NUXT_LISTMONK_AR_TEMPLATE_ID=
NUXT_LISTMONK_AR_TEMPLATE_ID=
#samplr#NUXT_LISTMONK_FR_TEMPLATE_ID=
NUXT_LISTMONK_FR_TEMPLATE_ID=
#samplr#NUXT_LISTMONK_CKB_TEMPLATE_ID=
NUXT_LISTMONK_CKB_TEMPLATE_ID=

# Redis configs - fallback to in-memory store if not configured
#samplr#NUXT_REDIS_HOST=localhost
NUXT_REDIS_HOST=localhost
#samplr#NUXT_REDIS_PORT=6379
NUXT_REDIS_PORT=6379
#samplr#NUXT_REDIS_PASSWORD=supersecret
NUXT_REDIS_PASSWORD=supersecret
#samplr#NUXT_REDIS_DB=0
NUXT_REDIS_DB=0
#samplr#NUXT_REDIS_KEY_PREFIX=nuha_auth:
NUXT_REDIS_KEY_PREFIX=nuha_auth:

# Base URL (update for production)
#samplr#NUXT_PUBLIC_BASE_URL=http://localhost:3000
NUXT_PUBLIC_BASE_URL=http://localhost:3000

# AI Model endpoint
#samplr#NUXT_AI_MODEL_URL=http://localhost:8000
NUXT_AI_MODEL_URL=http://localhost:8000

# Default AI Model/Region (optional - falls back to auto-detection if not set)
# NOTE: For the initial release of Nuha, set it to `egy`
#samplr#NUXT_PUBLIC_AI_MODEL_DEFAULT_REGION=egy
NUXT_PUBLIC_AI_MODEL_DEFAULT_REGION=egy

# Strapi Configuration
#samplr#NUXT_PUBLIC_STRAPI_URL=http://localhost:1337
NUXT_PUBLIC_STRAPI_URL=http://localhost:1337
#samplr#NUXT_STRAPI_TOKEN=
NUXT_STRAPI_TOKEN=
